{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "root": true,
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "semi": [2, "never"],
    "react/forbid-prop-types": 0,
    "no-continue": 0,
    "no-await-in-loop": 0,
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": 0
  }
}
